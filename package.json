{
  "name": "agbar-fh-kafka2kafka",
  "version": "0.1.0",
  "private": true,
  "description": "Firehose - Connection between Kafka broker and Kafka broker",
  "homepage": "https://www.mytra.es",
  "keywords": [
    "Typescript",
    "Nodejs",
    "language",
    "javascript",
    "firehose",
    "kafka"
  ],
  "repository": {
    "type": "git",
    "url": "https://devopmytra.visualstudio.com/P24020018%20-%20Aguas%20de%20Barcelona/_git/AGBAR-Firehose-Kafka2Kafka-Typescript"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Mytra Control S.L.",
  "main": "index.js",
  "scripts": {
    "clean:all": "rimraf \"./artifacts/**/{dist,*.md}\" \"./{test-results.xml,coverage,dist,reports}\" \"./*+(.tsbuildinfo)\"",
    "clean:dist": "rimraf \"./dist/**/{*.d.ts*,*.js.map,test}\"",
    "clean:distFolder": "rimraf dist",
    "test:licenses": "node ./.config/thirdPartyCheck.js",
    "test:jest": "jest --detectOpenHandles --config ./.config/jest.config.js",
    "test:mutants": "cross-env TS_NODE_FILES=true stryker run ./stryker.config.js",
    "test": "yarn run clean:all && yarn run compile:development && yarn run test:jest && yarn test:licenses",
    "thirdParty": "yarn run test:licenses",
    "doc:all": "yarn typedoc --options .config/typedoc.json --out docs/ src/",
    "run:dist": "node --trace-warnings -r dotenv/config ./dist/index.js",
    "run:debug": "node --nolazy --inspect-brk=9229 dist/index.js",
    "run:tests": "del tsconfig.tsbuildinfo && yarn run compile:development && yarn run run:dist",
    "compile": "tsc --build --clean && tsc --build",
    "compile:development": "tsc",
    "compile:production": "tsc -p ./tsconfig.production.json",
    "user:credentials": "vsts-npm-auth -config .npmrc",
    "files:licenses": "node ./.config/thirdPartyCheck.js --files",
    "files:obfuscate": "javascript-obfuscator ./dist  && renamer -f \"/-obfuscated/\" * \"dist/**\" --force",
    "files:copy:index": "copyfiles -V -f dist/index.js artifacts/linux/dist && copyfiles -V -f dist/index.js artifacts/windows/dist",
    "files:copy:dist": "copyfiles -V \"dist/**/*\" artifacts/linux && copyfiles -V \"dist/**/*\" artifacts/windows",
    "files:copy:readme": "copyfiles -V README.md artifacts/linux && copyfiles -V README.md artifacts/windows",
    "files:copy:license": "copyfiles -V -f .config/LICENSE.md artifacts/linux && copyfiles -V -f .config/LICENSE.md artifacts/windows",
    "files:copy": "yarn run files:copy:dist && yarn files:copy:index && yarn files:copy:readme && yarn files:copy:license",
    "files": "yarn run clean:dist && yarn run files:obfuscate && yarn run files:copy && yarn run files:licenses",
    "build:production": "yarn run clean:all &&  yarn run compile:production && yarn run files",
    "build:development": "yarn run clean:all && yarn run compile:development && yarn run files"
  },
  "dependencies": {
    "@mdf.js/core": "1.6.0-kafka-update.7",
    "@mdf.js/crash": "1.6.0-kafka-update.7",
    "@mdf.js/firehose": "1.6.0-kafka-update.7",
    "@mdf.js/kafka-provider": "1.6.0-kafka-update.7",
    "@mdf.js/logger": "1.6.0-kafka-update.7",
    "@mdf.js/service-registry": "1.6.0-kafka-update.7",
    "@mdf.js/utils": "1.6.0-kafka-update.7",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "joi": "^17.12.3",
    "long": "^5.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@types/debug": "^4.1.7",
    "@types/flat": "^5.0.2",
    "@types/jest": "29.5.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "20",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.4.3",
    "editorconfig": "^0.15.3",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-jest": "^28.8.0",
    "javascript-obfuscator": "^4.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jest-slow-test-reporter": "^1.0.0",
    "license-checker-rseidelsohn": "^3.1.0",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.16",
    "prettier": "^3.4.2",
    "renamer": "^4.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "29.2.5",
    "ts-node": "^10.7.0",
    "tslib": "^2.5.0",
    "typedoc": "^0.27.5",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=20.11.1"
  }
}
